{
    "name": "psr-easy/http-message",
    "description": "Simple http message implementation of PSR-3",
    "keywords": ["PSR-3", "Http", "Message"],
    "type": "library",
    "require": {
        "php": ">=5.5",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.5.0"
    },
    "autoload": {
        "psr-4": {
            "PsrEasy\\": "src/PsrEasy/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PsrEasy\\Tests\\": "tests/PsrEasy/Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    }
}
